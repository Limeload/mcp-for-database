{
  "name": "mcp-database-console",
  "version": "1.0.0",
  "description": "A revolutionary Next.js application that provides a natural language interface for database queries through the MCP-DB Connector with multiple concurrent connections support",
  "private": false,
  "keywords": [
    "hacktoberfest",
    "open-source",
    "nextjs",
    "database",
    "mcp",
    "typescript",
    "react",
    "natural-language",
    "sql",
    "contributors-welcome",
    "database-console",
    "query-interface",
    "sqlalchemy",
    "snowflake",
    "connection-pooling",
    "concurrent-connections",
    "database-management"
  ],
  "author": {
    "name": "Limeload",
    "email": "contact@limeload.com",
    "url": "https://github.com/Limeload"
  },
  "license": "MIT",
  "homepage": "https://github.com/Limeload/mcp-for-database#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Limeload/mcp-for-database.git"
  },
  "bugs": {
    "url": "https://github.com/Limeload/mcp-for-database/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "clean": "rm -rf .next out",
    "prepare": "npm run type-check",
    "db:pool:stats": "tsx scripts/connection-pool-stats.ts",
    "db:pool:cleanup": "tsx scripts/cleanup-connections.ts",
    "db:pool:cleanup:stats": "tsx scripts/cleanup-connections.ts --with-stats",
    "scripts:build": "tsc scripts/*.ts --outDir dist/scripts --target es2020 --module commonjs",
    "scripts:stats": "npm run scripts:build && node dist/scripts/connection-pool-stats.js",
    "scripts:cleanup": "npm run scripts:build && node dist/scripts/cleanup-connections.js"
  },
  "dependencies": {
    "mcp-handler": "^1.0.1",
    "next": "^15.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "redis": "^4.7.0",
    "zod": "^3.24.2",
    "uuid": "^9.0.1",
    "node-cron": "^3.0.3",
    "async-mutex": "^0.4.0",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^9.0.7",
    "@types/node-cron": "^3.0.11",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "ts-jest": "^29.1.1"
  },
  "packageManager": "pnpm@8.15.7+sha512.c85cd21b6da10332156b1ca2aa79c0a61ee7ad2eb0453b88ab299289e9e8ca93e6091232b25c07cbf61f6df77128d9c849e5c9ac6e44854dbd211c49f3a67adc",
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
